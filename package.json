{
    "version": "0.1.0-alpha.0",
    "description": "discord.js for node-red",
    "type": "commonjs",
    "license": "MIT",
    "keywords": [
        "node-red",
        "discord"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/eslym/node-red-discord.git"
    },
    "scripts": {
        "build": "rollup -c",
        "build:publish": "rollup -c && cd dist && yarn publish",
        "format": "prettier --write .",
        "debug": "rollup -c && node-red --settings ./node-red-settings.js",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@eslym/rs4r": "^2.0.0",
        "@rollup/plugin-typescript": "^11.1.2",
        "@schemastore/package": "^0.0.10",
        "@tsconfig/svelte": "^5.0.2",
        "@types/color": "^3.0.3",
        "@types/express": "^4.17.17",
        "@types/node": "^16",
        "@types/node-red": "^1.3.1",
        "color": "^4.2.3",
        "flatted": "^3.2.7",
        "husky": "^8.0.0",
        "jquery": "^3.7.0",
        "node-red": "^3.0.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "pretty-quick": "^3.1.3",
        "svelte": "^4.1.2",
        "svelte-preprocess": "^5.0.4",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "discord.js": "^14.11.0",
        "mustache": "^4"
    },
    "node-red": {
        "version": ">=3.0"
    },
    "engines": {
        "node": ">=16.9.0"
    }
}
