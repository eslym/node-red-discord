{
    "name": "@eslym/node-red-discord",
    "version": "1.0.0",
    "description": "discord.js for node-red",
    "type": "commonjs",
    "license": "MIT",
    "scripts": {
        "build": "sir",
        "format": "prettier --write .",
        "debug": "sir && node-red --userDir ./debug",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/node": "^16",
        "@types/node-red": "^1.3.1",
        "husky": "^8.0.0",
        "node-red": "^3.0.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "pretty-quick": "^3.1.3",
        "svelte-integration-red": "^1.14.3"
    },
    "dependencies": {
        "discord.js": "^14.11.0",
        "flatted": "^3.2.7"
    },
    "files": [
        "icons/**",
        "*.html",
        "*.js"
    ],
    "node-red": {
        "nodes": {
            "discord.client": "discord.client.js",
            "discord.event": "discord.event.js"
        }
    }
}
