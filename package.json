{
    "name": "@eslym/node-red-discord",
    "version": "0.0.1-alpha.2",
    "description": "discord.js for node-red",
    "type": "commonjs",
    "license": "MIT",
    "keywords": [
        "node-red",
        "discord"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/eslym/node-red-discord.git"
    },
    "scripts": {
        "build": "node sirex/build.js",
        "format": "prettier --write .",
        "debug": "sirex/build.js && node-red --userDir ./debug",
        "prepare": "husky install",
        "build:template": "node sirex/build-template.js"
    },
    "devDependencies": {
        "@handlebars/parser": "^2.1.0",
        "@rollup/plugin-commonjs": "^25.0.3",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@rollup/plugin-replace": "^5.0.2",
        "@types/color": "^3.0.3",
        "@types/node": "^16",
        "@types/node-red": "^1.3.1",
        "color": "^4.2.3",
        "flatted": "^3.2.7",
        "husky": "^8.0.0",
        "node-red": "^3.0.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "pretty-quick": "^3.1.3",
        "rollup": "^3.27.0",
        "rollup-plugin-import-css": "^3.3.1",
        "rollup-plugin-svelte": "^7.1.6",
        "svelte-integration-red": "^1.14.3"
    },
    "dependencies": {
        "discord.js": "^14.11.0",
        "mustache": "^4"
    },
    "files": [
        "resources/**",
        "dist/icons/**",
        "dist/*.html",
        "dist/**/*.js"
    ],
    "node-red": {
        "version": ">=3.0",
        "nodes": {
            "discord.client": "dist/discord.client.js",
            "discord.account": "dist/discord.account.js",
            "discord.event": "dist/discord.event.js",
            "discord.send": "dist/discord.send.js",
            "discord.filterBot": "dist/discord.filterBot.js",
            "discord.builder.message": "dist/discord.builder.message.js",
            "discord.builder.embed": "dist/discord.builder.embed.js",
            "discord.builder.component": "dist/discord.builder.component.js",
            "discord.presence.activity": "dist/discord.presence.activity.js"
        }
    },
    "engines": {
        "node": ">=16.9.0"
    }
}
