{
    "name": "@eslym/node-red-discord",
    "version": "0.0.1-alpha.0",
    "description": "discord.js for node-red",
    "type": "commonjs",
    "license": "MIT",
    "scripts": {
        "build": "node sirex/build.js",
        "format": "prettier --write .",
        "debug": "sirex/build.js && node-red --userDir ./debug",
        "prepare": "husky install",
        "build:template": "node sirex/build-template.js"
    },
    "devDependencies": {
        "@handlebars/parser": "^2.1.0",
        "@types/color": "^3.0.3",
        "@types/node": "^16",
        "@types/node-red": "^1.3.1",
        "husky": "^8.0.0",
        "mustache": "^4",
        "node-red": "^3.0.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "pretty-quick": "^3.1.3",
        "rollup-plugin-import-css": "^3.3.1",
        "svelte-integration-red": "^1.14.3"
    },
    "dependencies": {
        "color": "^4.2.3",
        "discord.js": "^14.11.0",
        "flatted": "^3.2.7"
    },
    "files": [
        "icons/**",
        "*.html",
        "*.js"
    ],
    "node-red": {
        "nodes": {
            "discord.client": "discord.client.js",
            "discord.account": "discord.account.js",
            "discord.event": "discord.event.js",
            "discord.send": "discord.send.js",
            "discord.builder.message": "discord.builder.message.js",
            "discord.builder.embed": "discord.builder.embed.js"
        }
    }
}
